{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Bugster",
  "logo": "/logo/LOGOTYPE-VIOLET-LIGHT.png",
  "description": "Bugster is an AI tester that simulates a real user in a real browser, providing insights directly in your PR. It's designed for developers at VC-backed startups who want to ship with confidence without writing or maintaining flaky test suites.",
  "colors": {
    "primary": "#5A5A5A",
    "dark": "#5A5A5A",
    "light": "#5A5A5A"
  },
  "appearance": {
    "default": "dark",
    "strict": true
  },
  "favicon": "/logo/logo2.jpeg",
  "navigation": {
    "tabs": [
      {
        "tab": "Documentation",
        "groups": [
          {
            "group": "Get Started",
            "pages": [
              "installation",
              "quickstart",
              "authentication"
            ]
          },
          {
            "group": "CLI Commands",
            "pages": [
              "commands/project-setup",
              "commands/test-generation",
              "commands/test-execution",
              "commands/test-maintenance",
              "commands/team-collaboration"
            ]
          },
          {
            "group": "Integrations",
            "pages": [
              "integrations/github"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Community",
          "href": "https://discord.gg/BG3vys73",
          "icon": "slack"
        },
        {
          "anchor": "Blog",
          "href": "https://www.bugster.dev/blog",
          "icon": "newspaper"
        }
      ]
    }
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "https://discord.gg/BG3vys73"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Start Testing",
      "href": "https://www.bugster.dev/"
    }
  },
  "footer": {
    "socials": {
      "github": "https://github.com/Bugsterapp/bugster-cli"
    }
  }
}